
public class Contractor {
	private String name;
	private String cbEmpList[] = new String[10];
	private static int countCbEmployees=0;
	public Contractor(String name) {
		this.name = name;
	}
	
	public  void addCbEmployee(String CbEmployee){
		this.cbEmpList[countCbEmployees] = CbEmployee;
	}
	
	public static getCbEmployees(){
		return countCbEmployees;
	}
}
