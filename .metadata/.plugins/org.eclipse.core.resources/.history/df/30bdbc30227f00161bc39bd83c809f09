class Employee{
 static int numberOfEmployee=0;
 private int id;
 private String firstName, lastName, salary, grade;
 private Date joiningDate;
 
 Employee(String fName,String lastName, String salary, String grade , Date date){
  numberOfEmployee++;
  this.id = numberOfEmployee;  
  this.firstName =  fName ;
  this.lastName = lastName;
  this.salary = salary;
  this.grade = grade;
  this.joiningDate = date;
 } 
void getNumberofEmployee(){
   System.out.println(numberOfEmployee);       
  } 
  void getName(){
   System.out.println(this.firstName+" "+ this.lastName);       
  } 
  void getSalary(){
   System.out.println(this.salary);       
  } 
  void getGrade(){
   System.out.println(this.grade);       
  }
  void getDate(){
   System.out.println(this.joiningMonth);       
  }    
}